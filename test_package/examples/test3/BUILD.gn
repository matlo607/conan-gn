#import("//basic-config/utils/unittest/unittest.gni")

group("test3_config") {
  testonly = true
  public_deps = [
    "//examples/lib/link_against/python:link_python",
    "//examples/lib/link_against/java:link_java",
    "//thirdparty/libs/googletest:gtest_main"
  ]
}

#unittest("test3") {
executable("test3") {
  testonly = true

  sources = [
    "test3.cpp",
  ]

  deps = [ ":test3_config" ]
}

generated_file("test3_env_metadata") {
  testonly = true
  outputs = [ "$root_build_dir/test3.json" ]
  data_keys = [ "envvars" ]
  deps = [ ":test3_config" ]
}